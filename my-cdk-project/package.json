{
  "name": "my-cdk-project",
  "version": "0.1.0",
  "bin": {
    "my-cdk-project": "bin/my-cdk-project.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk:deploy": "npm run build && cdk deploy",
    "watch": "tsc -w",
    "test": "jest --verbose",
    "cdk": "cdk"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": ["<rootDir>/test"],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.138",
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.2",
    "aws-cdk": "2.146.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.204.0",
    "@aws-cdk/aws-lambda": "^1.204.0",
    "aws-cdk-lib": "2.146.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
